 <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

 <meta name="google-site-verification" content="xODZ8KL-DLsbFrw_9mKJ5wMENfWI4O47W7sjt3UD0ZA" />
 <!-- Global site tag (gtag.js) - Google Analytics -->
 <script async src="https://www.googletagmanager.com/gtag/js?id=UA-50448281-7"></script>
 <script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-50448281-7');
</script>

</head>

<body>
	<%= render "shared/header" %>

	<div class="p-content">
		<div class="p-content__inner">
			<%= yield %>
		</div>
	</div>

  <%= render "shared/footer" %>
</body>
</html>

